/**
 * Spanky Corners -- CSS Rounded Corners by SitePoint
 *
 * Auto generated code
 * Details @ http://tools.sitepoint.com/spanky/
 * @version 1.1b
 */

/* all curved boxes get the bottom-left corner */
div.emrounded {
  background: #2d2c2c url(../images/bl_9_2d2c2c_121212.gif) no-repeat bottom left;
  position: relative;
  padding-top: 0;
  padding-bottom: 9px;
  margin: 1em 10px 0 0 ;
  opacity: 1;
  filter:alpha(opacity=1);
  
}

/* all elements in 'div.rounded' gets the B-R corner */
div.emrounded .emroundedbody {
  background: #2d2c2c url(../images/br_9_2d2c2c_121212.gif) no-repeat bottom right;
  background-color: transparent;
  color: #dddddd;
  padding: 0;
  margin: 0 0 -9px 0px;
}


/* Set up some H's as 'box heads' with the T-R and T-L corners */
#emcontainer div.emrounded h4 {
  background: #2d2c2c url(../images/tr_9_2d2c2c_121212.gif) no-repeat top right;
  display: list-item;
  list-style-image: url(../images/tlc_9_2d2c2c_121212.gif);
  list-style-position: inside;
  list-style-type:none;
  padding: 0;
  margin: 0;
  position: relative;
  line-height: 9px;
  height: 9px;
}

#emcontainer #emdropdown
{
	background: #ffffff url(../images/backgrounds/dropdown_bg.gif) no-repeat top right;
	margin-right:14px;
	position: relative;
	height: 20px;
	min-width: 80px;
	cursor: pointer;
}
#emcontainer #emdropdown .emvalue
{
	color: black;
	height: 13px;
	overflow: hidden;
	margin-right: 22px;
	padding: 2px 14px 1px 10px;
}

#emdropdowndiv
{
	z-Index:600;
	position:absolute;
	background-color: white;
	border: 1px solid #222222;
	display: hidden;
	min-width: 250px;
	
}

